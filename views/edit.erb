<h1 class="center">Edit <%= @order.title %></h1>
<form method="POST" action="/apps/<%= @order.id %>">
  <input type="hidden" name="_method" value="PUT">
  <div class="input-field">
    <input id="title" required name="title" value="<%= @order.title %>" />
    <label class="active" for="title">Your New Choice</label>
  </div>
 <button class="btn right" type="submit">Save</button>
</form>
